<proxy xmlns="http://ws.apache.org/ns/synapse" name="ChainingTestProxy" transports="https http" startOnLoad="true" trace="disable">
        <description/>
        <target>
            <inSequence>
                <send>
                    <endpoint>
                        <http method="post" uri-template="http://localhost:8280/services/Service1"/>
                    </endpoint>
                </send>
            </inSequence>
            <outSequence>
                <call>
                    <endpoint>
                        <address uri="http://localhost:8280/services/Service2"/>
                    </endpoint>
                </call>
                <call>
                    <endpoint>
                        <address uri="http://localhost:8280/services/Service3"/>
                    </endpoint>
                </call>
                <log level="full">
                    <property name="MSG" value="Payload after service 3"/>
                </log>
                <respond/>
            </outSequence>
        </target>
    </proxy>